<% include tmpl_order_header %>

<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
  	<h1 class="page-header">Dishes</h1>
  	<div class="row placeholders">
  	  <% dishes.forEach(function(dish){ %>
  	  	<div class="col-xs-4 col-sm-4 col-md-3 col-lg-2 placeholder">
  	  		<h3>rate: <span class="rate"><%= dish.rate %></span></h3>
	   		<img class='img-responsive' src='/images/food.jpeg' width=200 height=200>
			<h4><%= dish.name %></h4>
			<span class="text-muted">price: <%= dish.price %></span>
        </div>
  	  <% }) %>
  	</div>
</div>
<% include tmpl_order_tailer %>
